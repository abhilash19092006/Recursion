package Recursion;

public class PrintNbitbinarynumbers {
	 public static void generateBinary(String str, int n, int ones, int zeros) {
	        if (str.length() == n) {
	            if (ones >= zeros) {
	                System.out.println(str);
	            }
	            return;
	        }
	        
	        generateBinary(str + "1", n, ones + 1, zeros);
	        generateBinary(str + "0", n, ones, zeros + 1);
	    }
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		generateBinary("",4,0,0);
	}

}
